{
  "name": "cp-olympic-backend",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node .",
    "lint": "eslint",
    "db.create": "docker create --name cp-olympic-db --env-file .env -p 5432:5432 bitnami/postgresql",
    "db.start": "docker start -i cp-olympic-db",
    "db.generate": "npx prisma generate",
    "db.push": "npx prisma db push",
    "db.studio": "npx prisma studio",
    "db.migrate": "npx prisma migrate dev",
    "db.apply": "npx prisma migrate deploy",
    "db.seed": "tsx prisma/seed.ts",
    "db.reset": "npx prisma migrate reset"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.3",
    "zod": "^4.2.1",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24",
    "eslint": "^9.39.2",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "engines": {
    "node": "^24"
  }
}
